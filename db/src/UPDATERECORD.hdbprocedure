PROCEDURE "UPDATERECORD"(ID INTEGER,vTITLE NVARCHAR(200), vSTOCK INTEGER, out result "MY_BOOKSHOP_BOOKS")
    LANGUAGE SQLSCRIPT
    SQL SECURITY INVOKER
    -- DEFAULT SCHEMA <default_schema_name>

AS 
BEGIN


    UPDATE  "MY_BOOKSHOP_BOOKS" SET TITLE = vTITLE, STOCK = vSTOCK
            WHERE ID = :ID;

    result = SELECT * FROM "MY_BOOKSHOP_BOOKS" where ID = :ID;
END 