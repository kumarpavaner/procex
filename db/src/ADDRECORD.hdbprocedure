PROCEDURE "ADDRECORD"(ID INTEGER,TITLE NVARCHAR(200), STOCK INTEGER, out result "MY_BOOKSHOP_BOOKS")
    LANGUAGE SQLSCRIPT
    SQL SECURITY INVOKER
    -- DEFAULT SCHEMA <default_schema_name>

AS 
BEGIN


    INSERT INTO "MY_BOOKSHOP_BOOKS"("ID","TITLE","STOCK")
            VALUES(ID,TITLE,STOCK);

    result = SELECT * FROM "MY_BOOKSHOP_BOOKS" where ID = :ID;
END 